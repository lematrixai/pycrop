{
  "expo": {
    "name": "pycrop",
    "slug": "pycrop",
    "version": "1.0.0",
    "orientation": "portrait",
    "backgroundColor": "#447055",
    "icon": "./assets/images/tomato.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#447055"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://pycrop.vercel.app/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#447055"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
